{
  "cli": {
    "version": ">= 3.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "distribution": "store"
      }
    },
    "testflight": {
      "environment": "production",
      "env": {
        "PRIVY_APP_ID": "${PRIVY_APP_ID}",
        "PRIVY_CLIENT_ID": "${PRIVY_CLIENT_ID}",
        "SENTRY_DSN": "${SENTRY_DSN}",
        "POSTHOG_API_KEY": "${POSTHOG_API_KEY}",
        "POSTHOG_HOST": "${POSTHOG_HOST}",
        "EXPO_PUBLIC_AI_PRISE_MODE": "${EXPO_PUBLIC_AI_PRISE_MODE}",
        "AIPRISE_TEMPLATE_ID": "${AIPRISE_TEMPLATE_ID}",
        "RPC_SEPOLIA_URL": "${RPC_SEPOLIA_URL}",
        "PAYMASTER_SEPOLIA_URL": "${PAYMASTER_SEPOLIA_URL}",
        "RPC_BASE_URL": "${RPC_BASE_URL}",
        "PAYMASTER_BASE_URL": "${PAYMASTER_BASE_URL}",
        "EXPO_PUBLIC_ENVIRONMENT": "${EXPO_PUBLIC_ENVIRONMENT}",
        "EXPO_PUBLIC_API_URL": "${EXPO_PUBLIC_API_URL}"
      },
      "extends": "production",
      "channel": "testflight",
      "distribution": "internal"
    },
    "playstore-beta": {
      "env": {
        "PRIVY_APP_ID": "${PRIVY_APP_ID}",
        "PRIVY_CLIENT_ID": "${PRIVY_CLIENT_ID}",
        "SENTRY_DSN": "${SENTRY_DSN}",
        "POSTHOG_API_KEY": "${POSTHOG_API_KEY}",
        "POSTHOG_HOST": "${POSTHOG_HOST}",
        "EXPO_PUBLIC_AI_PRISE_MODE": "${EXPO_PUBLIC_AI_PRISE_MODE}",
        "AIPRISE_TEMPLATE_ID": "${AIPRISE_TEMPLATE_ID}",
        "RPC_SEPOLIA_URL": "${RPC_SEPOLIA_URL}",
        "PAYMASTER_SEPOLIA_URL": "${PAYMASTER_SEPOLIA_URL}",
        "RPC_BASE_URL": "${RPC_BASE_URL}",
        "PAYMASTER_BASE_URL": "${PAYMASTER_BASE_URL}",
        "EXPO_PUBLIC_ENVIRONMENT": "${EXPO_PUBLIC_ENVIRONMENT}",
        "EXPO_PUBLIC_API_URL": "${EXPO_PUBLIC_API_URL}"
      },
      "extends": "production",
      "channel": "beta",
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production",
        "releaseStatus": "completed"
      }
    },
    "playstore-beta": {
      "android": {
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
